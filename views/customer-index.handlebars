<h1 class="text-center">Register</h1>
<div class="container">
    <a href="/create" class="btn btn-primary">+ New customer</a>
    <table class="table table-striped mt-4">
        <tr>
            <th>id</th>
            <th>name</th>
            <th>user_name</th>
            <th>email</th>
            <th>password</th>
            <th>mobile_number</th>
            <th>address</th>
            <th>dob</th>
        </tr>
        {{#each data}}
        <tr>
            <td>{{this.dataValues.id}}</td>
            <td>{{this.dataValues.name}}</td>
            <td>{{this.dataValues.user_name}}</td>
            <td>{{this.dataValues.email}}</td>
            <td>{{this.dataValues.password}}</td>
            <td>{{this.dataValues.mobile_number}}</td>
            <td>{{this.dataValues.address}}</td>
            <td>{{this.dataValues.dob}}</td>
            <td>
                <a href="/update/{{this.dataValues.id}}">Edit</a> |
                <a href="/delete/{{this.dataValues.id}}" class="text-danger" onclick="return confirm('Are you sure you want to delete this')">Delete</a>
            </td>
        </tr>
        {{/each}}
    </table>
</div>